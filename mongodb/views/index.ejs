<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Products</title>
</head>
<body>
    <h1>Product List</h1>
    <ul>
        <% arr.forEach(function(product) { %>
            <li>
                <h2><%= product.name %></h2>
                <p>Price: $<%= product.price %></p>
                <p>Description: <%= product.description %></p>
            </li>
        <% }) %>
    </ul>
</body>
</html>

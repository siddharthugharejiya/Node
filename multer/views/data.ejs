    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
    </head>
    <body>
    <h1>data</h1>
    <% data.map((el)=>{ %>
        <h1><%=el._id%></h1>
        <img src="/image/<%=el.image%>" alt="" width="200px">
        <a href="/delete/<%=el._id%>">Delete</a>
        <a href="/edit/<%=el._id%>">Edit</a>
    <% }) %>
    </body>
    </html>